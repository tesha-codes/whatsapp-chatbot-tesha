{
  "containerDefinitions": [
    {
      "name": "tesha-app",
      "image": "825172087354.dkr.ecr.af-south-1.amazonaws.com/tesha-app",
      "cpu": 0,
      "portMappings": [
        {
          "name": "tesha-app-9000-tcp",
          "containerPort": 9000,
          "hostPort": 9000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/tesha-app-task-definition",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "af-south-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "tesha-app-task-definition",
  "taskRoleArn": "arn:aws:iam::825172087354:role/ECSTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::825172087354:role/ECSTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
